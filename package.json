{
  "name": "@wry-smile/vite-plugin-global-variable",
  "type": "module",
  "version": "1.0.3",
  "description": "vite-plugin, global variable",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "playground": "cd ./playground/vue3 && npm run dev",
    "release": "npm run build && npx releaseKit ./package.json && npm publish --access=public"
  },
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./*": "./*",
    "./tools": {
      "import": "./tools/index.js",
      "types": "./tools/index.d.ts"
    }
  },
  "files": [
    "dist",
    "tools"
  ],
  "keywords": [
    "vite-plugin",
    "env",
    "environment",
    "global variable"
  ],
  "author": "@wry-smile",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@wry-smile/eslint-config-vue": "^0.1.14",
    "@wry-smile/release-kit": "^1.1.0",
    "chalk": "^5.2.0",
    "eslint": "^8.34.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.3"
  },
  "dependencies": {
    "@wry-smile/utils": "^1.1.2"
  }
}
